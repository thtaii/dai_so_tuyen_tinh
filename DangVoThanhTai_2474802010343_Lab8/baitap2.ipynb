   "source": [
    "import numpy as np\n",
    "\n",
    "# Tập điểm F của chữ F gốc\n",
    "F = np.array([[0,0,3,3,1,1,2,2,1,1],[0,5,5,4,4,3,3,2,2,0]])\n",
    "\n",
    "# Biến đổi: (x, y) -> (y, x - y)\n",
    "def transform(point):\n",
    "    x, y = point\n",
    "    return [y, x - y]\n",
    "\n",
    "transformed = np.array([transform(p) for p in F.T]).T\n",
    "print(transformed)\n"
   ]
  }
